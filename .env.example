# Example .env.example — copiar para `.env.local` e preencher
# NÃO comitar variáveis sensíveis em repositórios públicos

# Next/Auth
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=change_this_to_a_strong_random_value

# S3 / Storage (opcional)
NEXT_PUBLIC_S3_BUCKET=your-bucket-name
S3_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key

# Ambiente
NODE_ENV=development

# Configurações públicas que a app pode ler (prefixo NEXT_PUBLIC_)
NEXT_PUBLIC_APP_NAME="AIX Dance"
NEXT_PUBLIC_API_BASE=/api

# Ajuste ou acrescente variáveis conforme a infraestrutura do projeto